alias dump with open('.dump.html', 'w') as f: f.write(%1)
alias dumpb with open('.dump.html', 'w') as f: f.write(%1.body_as_unicode())
alias ipython import IPython; IPython.embed()
alias ipy ipython
alias view from scrapy.utils.response import open_in_browser; open_in_browser(%1)
alias bye import os; os.system('kill -9 %d' % os.getpid())
